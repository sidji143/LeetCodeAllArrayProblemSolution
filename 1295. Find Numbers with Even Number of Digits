class Solution {
    public int findNumbers(int[] nums) {
        int count =0;
        String arr[] = new String[nums.length];
        
        for(int i =0; i< arr.length;i++){
            arr[i] = String.valueOf(nums[i]); 
        }
        String len="";
        
        for(int i=0; i < arr.length;i++){
            len = arr[i];
                int e = len.length();
                    if(e % 2 ==0)
                count++;
            }
        return count;
        }
        
    }
